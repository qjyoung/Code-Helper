<idea-plugin>
    <id>com.siyueren.codehelper</id>
    <name>Code Helper</name>
    <version>1.3</version>
    <vendor email="jianyongqiao@qq.com" url="http://blog.qjyoung.top">qjyoung</vendor>
    
    <description><![CDATA[
      Help you generate setter call automatically And Free Your Hands<br>
      <a href="http://blog.qjyoung.top">My Blog</a>.<br>
      <br>
      <a href="https://github.com/qjyoung/Code-Helper">https://github.com/qjyoung/Code-Helper</a>
      <br>
      自动生成目标对象的方法调用，解放双手！<br>
      <a href="http://blog.qjyoung.top">查看我的博客</a>.<br>
    ]]></description>
    
    <change-notes><![CDATA[
      version 1.3<br>
      fix bug
    ]]>
    </change-notes>
    
    <!--<idea-version since-build="173.0"/>-->
    <idea-version since-build="131"/>
    
    <extensions defaultExtensionNs="com.intellij">
        <projectService serviceImplementation="com.siyueren.codehelper.service.CodeService"/>
        <intentionAction>
            <className>com.siyueren.codehelper.intention.GenerateSetterCallsIntention</className>
        </intentionAction>
        
    </extensions>
    
    <actions>
        <!-- Add your actions here -->
